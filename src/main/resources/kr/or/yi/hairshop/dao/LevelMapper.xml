<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.yi.hairshop.dao.LevelMapper">

	<resultMap type="Level" id="basicLevel">
		<result property="lGrade" column="l_grade" />
		<result property="lSale" column="l_sale" />
	</resultMap>

	<select id="selectLevelByAll" resultType="Level" resultMap="basicLevel">
		SELECT l_grade, l_sale
		FROM hairshop.level;
	</select>
	
	

